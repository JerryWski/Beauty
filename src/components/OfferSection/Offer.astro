<style>
.offer-section {
    background-color: white;
    padding: 20px;
    display: flex;
    flex-direction: column;
    align-items: center;
    }
.offer-header {
    margin-top: 40px;
    margin-bottom: 40px;
    padding: 20px 0 0px 0px;
    font-family: var(--logo-font);
    color: var(--lightgrey);
    text-align: center;
    letter-spacing: 0.5px;
    font-weight: 700;
    font-size: clamp(2.2rem, 4vw, 3.2rem);
}
.offer-underline {
    display: flex;
    height: 1px;
    width: 60%;
    background-color: var(--main-pink);
    margin-bottom: 30px;
}
.offer-container {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  margin-bottom: 50px;
  gap: 50px;
}
.offer {
    margin: 5px;
  background-color: var(--lightrose);
  border: 1px solid var(--main-pink);
  border-radius: 5px;
  display: flex;
  text-align:center;
  justify-content: space-around;
  align-items: center;
  flex-direction: column;
  font-family: var(--main-font);
  color: var(--black);
  height: 420px;
  max-width: 350px;
  padding: 20px;
}

</style>
